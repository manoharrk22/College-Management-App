        <activity
            android:name=".AdminLoginActivity"
            android:label="AdminLogin"
            android:windowSoftInputMode="stateHidden"/>
        <activity
            android:name=".CategoryStudentActivity"
            android:label="CategoryStudent" />
        <activity
            android:name=".CategoryAdminActivity"
            android:label="CategoryAdmin" />
        <activity
            android:name=".AddToGalleryActivity"
            android:label="AddToGalleryActivity" />
