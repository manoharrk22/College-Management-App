<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.collegemanagementBmsit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollegeManagement">
        <activity android:name=".staffdetails"></activity>
        <activity android:name=".adminmail" />
        <activity android:name=".studentmail" />
        <activity android:name=".viewnotice" />
        <activity android:name=".showuploads" />
        <activity android:name=".addnotice" />
        <activity android:name=".adminregister" />
        <activity android:name=".studentregister" />
        <activity
            android:name=".studentLoginActivity"
